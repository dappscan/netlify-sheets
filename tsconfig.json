
{
  "extends": "astro/tsconfigs/base"

     "compilerOptions": {
       "target": "ESNext",
       "module": "commonjs",
       "strict": true,
       "esModuleInterop": true,
       "skipLibCheck": true,
       "forceConsistentCasingInFileNames": true,
       "outDir": "./dist",
       "baseUrl": "./",
       "typeRoots": [
         "./node_modules/@types",
         "./types"
       ]
     },
     "include": ["netlify/functions/**/*"]
   }
